package Notas;

import java.util.Scanner;

public class TesteAluno {

	public static void main(String[] args) {
		Aluno a = new Aluno();
		Aluno b = new Aluno();
		
		Scanner sc = new Scanner(System.in);
		
		a.entradaNome();
	    a.media();
		b.entradaNome();
		b.media();
		

		a.imprimir();
		a.calculo();
		b.imprimir();
		b.calculo();
		
	}

}
